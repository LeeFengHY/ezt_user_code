<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="@dimen/height_45"
        android:gravity="center"
        android:layout_weight="1">
        <ImageView
            android:id="@+id/new_fragment_ranking_list_item_order_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/new_ranking_glod"
            android:visibility="visible"/>
        <TextView
            android:id="@+id/new_fragment_ranking_list_item_order_text"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/text_size_16"
            android:textColor="@color/text_color_deep_gray"
            android:gravity="center"
            android:visibility="gone"
            android:text="4"/>
    </LinearLayout>

    <TextView
        android:id="@+id/new_fragment_ranking_list_item_name"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:textSize="@dimen/text_size_16"
        android:textColor="@color/text_color_deep_gray"
        android:gravity="center"
        android:text="李医生"/>
    <TextView
        android:id="@+id/new_fragment_ranking_list_item_count"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:textSize="@dimen/text_size_16"
        android:textColor="@color/text_color_deep_gray"
        android:gravity="center"
        android:text="600"/>
    <LinearLayout

        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_weight="1">
        <ImageView
            android:id="@+id/new_fragment_ranking_list_item_isAdd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/new_ranking_add"/>
    </LinearLayout>
</LinearLayout>
