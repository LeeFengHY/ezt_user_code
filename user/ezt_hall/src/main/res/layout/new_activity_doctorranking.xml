<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
    android:background="@color/white">
<include layout="@layout/new_activity_title_bar"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_35">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/new_activity_doctor_rank_week"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="本周排名"
                android:layout_weight="1"
                android:gravity="center"
                android:textColor="@color/border_line"
                android:textSize="@dimen/text_size_16"/>
            <TextView
                android:id="@+id/new_activity_doctor_rank_week_bottom"
                android:layout_width="@dimen/line_width_70"
                android:layout_height="@dimen/line_height_1"
                android:layout_gravity="center_horizontal"
                android:background="@color/border_line"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/new_activity_doctor_rand_month"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="本月排名"
                android:layout_weight="1"
                android:gravity="center"
                android:textColor="@color/text_color_deep_gray"
                android:textSize="@dimen/text_size_16"/>
            <TextView
                android:id="@+id/new_activity_doctor_rank_month_bottom"
                android:layout_width="@dimen/line_width_70"
                android:layout_height="@dimen/line_height_1"
                android:layout_gravity="center_horizontal"
                android:visibility="invisible"
                android:background="@color/border_line"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/new_activity_doctor_rank_year"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="年度排名"
                android:layout_weight="1"
                android:gravity="center"
                android:textColor="@color/text_color_deep_gray"
                android:textSize="@dimen/text_size_16"/>
            <TextView
                android:id="@+id/new_activity_doctor_rank_year_bottom"
                android:layout_width="@dimen/line_width_70"
                android:layout_height="@dimen/line_height_1"
                android:layout_gravity="center_horizontal"
                android:visibility="invisible"
                android:background="@color/border_line"/>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_height_0.5"
        android:background="@color/line_color_gray"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_45">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="排名"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/text_color_deep_gray"
            android:textSize="@dimen/text_size_16"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="医生"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/text_color_deep_gray"
            android:textSize="@dimen/text_size_16"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="预约量"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/text_color_deep_gray"
            android:textSize="@dimen/text_size_16"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="排行变化"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/text_color_deep_gray"
            android:textSize="@dimen/text_size_16"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    </LinearLayout>
</LinearLayout>
